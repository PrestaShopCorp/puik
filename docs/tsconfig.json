    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "types": ["vitepress/client"]
  },
  "include": ["./src/**/*", "./.vitepress/**/*"]
}

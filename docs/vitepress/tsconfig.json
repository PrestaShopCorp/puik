{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*",
    ".vitepress/components/*.vue",
    ".vitepress/theme/*.ts",
    ".vitepress/plugins/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".vitepress/dist",
    ".vitepress/cache"
  ],
  "vueCompilerOptions": {
    "vitePressExtensions": [".md"],
  },
}

/* @import './common/typography.postcss'; */

.puik-rating-card {
    @apply flex items-center space-x-1;
    &_stars-container {
        @apply flex space-x-1;
    }
    &_average-value {
        @extend .puik-body-default-bold;
    }
    &_total-ratings {
        @extend .puik-body-small-medium;
    }
}
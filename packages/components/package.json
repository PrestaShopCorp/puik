{
  "name": "@prestashopcorp/puik-components",
  "version": "0.0.1",
  "type": "module",
  "description": "all components are settled here",
  "scripts": {
    "build": "vite build"
  },
  "peerDependencies": {
    "vue": "^3.3.7"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.11",
    "@popperjs/core": "^2.11.6",
    "@prestashopcorp/puik-theme": "workspace:*",
    "@vueuse/core": "^9.13.0"
  },
  "devDependencies": {
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-define-options": "^1.2.3",
    "vue": "^3.3.7",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "default": "./dist/index.mjs",
          "types": "./dist/index.d.ts"
        },
        "require": {
          "default": "./dist/index.cjs",
          "types": "./dist/index.d.ts"
        }
      },
      "./es": "./dist/index.mjs",
      "./lib": "./dist/index.cjs",
      "./es/*.mjs": "./dist/*.mjs",
      "./es/*": "./dist/*.mjs",
      "./lib/*.cjs": "./dist/*.cjs",
      "./lib/*": "./dist/*.cjs",
      "./*": "./dist/*"
    }
  }
}
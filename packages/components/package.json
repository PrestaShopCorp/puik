{
  "name": "@puik/components",
  "version": "0.0.1",
  "description": "all components are settled here",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./es": "./dist/index.js",
    "./es/*.mjs": "./dist/*.js",
    "./es/*": "./dist/*.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "build:lib": "rimraf dist && rollup -c",
    "gen:dts": "vue-tsc --declaration --emitDeclarationOnly --outDir ./dist/",
    "build": "pnpm build:lib && pnpm gen:dts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@headlessui/vue": "^1.7.11",
    "@popperjs/core": "^2.11.6",
    "@puik/locale": "workspace:*",
    "@puik/theme": "workspace:*",
    "@puik/utils": "workspace:*",
    "@vueuse/core": "^9.13.0"
  },
  "devDependencies": {
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-define-options": "^1.2.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0"
  }
}

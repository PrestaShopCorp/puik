{
  "name": "@prestashopcorp/puik-components",
  "version": "0.0.1",
  "description": "all components are settled here",
  "main": "./lib/index.js",
  "module": "./es/index.mjs",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/es/index.mjs",
        "types": "./dist/es/index.d.ts"
      },
      "require": {
        "default": "./dist/lib/index.js",
        "types": "./dist/lib/index.d.ts"
      }
    },
    "./es": "./dist/es/index.mjs",
    "./lib": "./dist/lib/index.js",
    "./es/*.mjs": "./dist/es/*.mjs",
    "./es/*": "./dist/es/*.mjs",
    "./lib/*.js": "./dist/lib/*.js",
    "./lib/*": "./dist/lib/*.js",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:lib": "rimraf dist && rollup -c",
    "gen:esm-dts": "vue-tsc --p tsconfig.build.json --declaration --emitDeclarationOnly --outDir ./dist/es",
    "gen:cjs-dts": "vue-tsc --p tsconfig.build.json --declaration --emitDeclarationOnly --outDir ./dist/lib",
    "build": "pnpm build:lib && pnpm gen:esm-dts && pnpm gen:cjs-dts"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.11",
    "@popperjs/core": "^2.11.6",
    "@prestashopcorp/puik-locale": "workspace:*",
    "@prestashopcorp/puik-theme": "workspace:*",
    "@prestashopcorp/puik-utils": "workspace:*",
    "@vueuse/core": "^9.13.0"
  },
  "devDependencies": {
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-define-options": "^1.2.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0"
  }
}
